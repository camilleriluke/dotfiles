// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.wordWrap": "off",
    "editor.minimap.enabled": true,
    "editor.formatOnPaste": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/static/**/*.js": false,
        "**/static/**/*.css": false,
        "**/**/*.pyc": true,
        "**/*.pyo": true,
        "**/*.exe": true,
        "**/*.dll": true,
        "**/*.obj": true,
        "**/*.o": true,
        "**/*.a": true,
        "**/*.lib": true,
        "**/*.so": true,
        "**/*.dylib": true,
        "**/*.ncb": true,
        "**/*.sdf": true,
        "**/*.suo": true,
        "**/*.pdb": true,
        "**/*.idb": true,
        "**/.DS_Store": true,
        "**/*.class": true,
        "**/*.psd": true,
        "**/*.db": true,
        "**/*.sublime-workspace": true,
        "**/npm-shrinkwrap.json": true,
        "**/npm-debug.log": true,
        "**/yarn.lock": true,
        "**/.bundle": true,
        "**/.svn": true,
        "**/.git": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/node_modules": true,
        "**/node_modules-old": true,
        "**/node_modules_old": true,
        "**/build": true,
        "**/dist": true,
        "**/.sass-cache": true,
        "**/env": true,
        "**/.virtualenv": true,
        "**/.githooks": true,
        "**/.vagrant": true,
        "**/.venv": true,
        "**/__pycache__": true,
        "**/tmp": true,
        "**/es5": true,
        "**/.tox": true,
        "**/var/": true,
        "**/_deploy": true,
        "**/.pygments-cache": true,
        "**/static/wagtailadmin": true,
        "**/coverage_html_report": true,
        "**/coverage": true,
        "**/coverage/lcov-report": true,
        "**/*.egg-info": true,
        "**/www": true,
        "**/css_regression_data": true,
        "**/_site": true
    },
    "window.reopenFolders": "all",
    "window.zoomLevel": 0,
    "terminal.external.osxExec": "iTerm.app",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "markdownlint.config": {
        "MD003": false,
        "MD013": false,
        "MD024": false,
        "MD034": false
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.showIcons": false,
    "workbench.editor.openPositioning": "right",
    "workbench.sideBar.location": "left",
    "html.format.wrapLineLength": 0,
    "html.format.extraLiners": "object",
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "editor.tabSize": 4,
    "git.enabled": true,
    "git.autorefresh": true,
    "git.autofetch": true,
    "git.countBadge": "all",
    "css.validate": true,
    "scss.validate": true,
    "workbench.iconTheme": "seti",
    "jsAutolint.defaultLinters": ["eslint", "standard"],
    "jsAutolint.enable": true,
    "typescript.check.npmIsInstalled": false
}
